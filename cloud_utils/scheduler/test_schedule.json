[
  {
    "schedule": "0 8 * * *",
    "run": {
      "pod_name": "insight-reports-test",
      "image": "registry.gitlab.com/airbud/analysis",
      "env_variables": [
        {
          "name": "GOOGLE_APPLICATION_CREDENTIALS",
          "value": "/var/secrets/gcp/credentials.json"
        },
        {
          "name": "STORAGE_PROVIDER",
          "value": "azure"
        },
        {
          "name": "TEST_SECRET_ENV",
          "value": "TOKEN"
        }
      ],
      "secrets": [
        {
          "data": {
            "credentials.json": ""
          },
          "mount_path": "/var/secrets/gcp",
          "secret_name": "prod-jupyter-gcp-credentials",
          "volume_name": "gcp-credentials"
        }
      ],
      "command": [
        "python",
        "default_insight_reports.py"
      ],
      "args": [],
      "labels": {
        "testlabel": "some-label"
      },
      "resources": {
        "requests": {
          "cpu": "1",
          "memory": "5Gi"
        }
      },
      "env_from_secrets": [
        "TEST_SECRET_PROVIDER_ENV"
      ],
      "secret_provider_class": "test-secrets-provider"
    }
  }
]
